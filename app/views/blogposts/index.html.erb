<h1 class="display-4 text-primary">Blogposts#index</h1>
<p>※記事投稿の一覧</p>
<table class="table">
  <tr>
    <th>id</th>
    <th>title</th>
    <th>read</th>
    <th>genre</th>
    <th colspan="2"></th>
  </tr>
  <% @data.each do |obj| %>
    <tr>
      <td><%= obj.id %></td>
      <td><%= obj.title %></td>
      <td><%= obj.read %></td>
      <td><%= obj.bloggenre.name %></td>
      <td><a href="/blogposts/edit/<%= obj.id %>">Edit</a></td>
      <td><a href="/blogposts/delete/<%= obj.id %>">Delete</a></td>
    </tr>
  <% end %>
</table>
<P><a href="/blogposts/add">新規作成 ＞＞</a></P>